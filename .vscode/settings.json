{
  "editor.tabSize": 2,
  "editor.suggest.shareSuggestSelections": true,
  "editor.accessibilityPageSize": 8,
  "window.zoomLevel": -1,
  "editor.mouseWheelZoom": true,
  "editor.parameterHints.enabled": false,
  "eslint.probe": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact",
    "html",
    "vue",
    "markdown",
    "react"
  ],
  "eslint.quiet": true,
  "workbench.colorCustomizations": {
    "activityBar.background": "#5B0A2F",
    "titleBar.activeBackground": "#7F0F41",
    "titleBar.activeForeground": "#FFFCFD"
  }
}